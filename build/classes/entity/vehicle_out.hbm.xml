<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entity.VehicleOut" table="vehicle_out">
		<id name="plateNumber" column="plateNumber"></id>
		<property name="borrower" column="borrower" />
		<property name="reason" column="reason" />
		<property name="startTime" column="startTime" />
		<property name="endTime" column="endTime" />
	</class>
</hibernate-mapping>